<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://ads.sj.ifsc.edu.br/pbe"
           targetNamespace="http://ads.sj.ifsc.edu.br/pbe" elementFormDefault="qualified">

    <xs:element name="somaRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="i1" type="xs:int"/>
                <xs:element name="i2" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="somaResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="somaReturn" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>